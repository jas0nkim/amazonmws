{
    "name": "ETOA Ordering",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "ETOA ordering app",
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [
                "js/components/jquery/dist/jquery.min.js",
                "js/contentscript.js"
            ],
            "run_at": "document_end"
        }
    ],

    "content_security_policy": "script-src 'self'; object-src 'self'"
}